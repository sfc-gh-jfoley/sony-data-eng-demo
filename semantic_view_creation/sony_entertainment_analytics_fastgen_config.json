      {
        "sql_text": "SELECT title_name, franchise, genre, imdb_score, release_year FROM SONY_DE.GOLD.DT_DIM_TITLES WHERE genre = 'Action'"
      },
      {
        "sql_text": "SELECT account_type, region, country_code, lifetime_interactions FROM SONY_DE.GOLD.DT_DIM_FANS WHERE account_type = 'VERIFIED'"
